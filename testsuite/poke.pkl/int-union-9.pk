/* { dg-do compile } */


type Foo =
  union int<16>
  {
    union int<16> { int<16> a : a < 0; offset<uint<16>,B> b; } la;
    offset<int<16>,MiB> c;

    method _print = void:
      {
        if (!(la.a ?! E_elem))
          printf "%v", la.a;
        if (!(la.b ?! E_elem))
          printf "%v\n", la.b;
        if (!(c ?! E_elem))
          printf "%v\n", c;
      }
  };
