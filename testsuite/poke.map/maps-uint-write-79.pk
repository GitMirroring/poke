/* { dg-do run } */

var m1 = -1UL as uint<48>;

fun test = (offset<int<64>,b> off) int<32>:
  {
    var fd = open ("*mem*");

    uint<48> @ fd : off = m1;

    var ok = (uint<48> @ fd : off) == m1;

    close (fd);
    return ok;
  }

