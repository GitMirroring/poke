/* MIPS PE based relocations.  */

var PE_MIPS_R_BASED_JMPADDR = 0x05UN,
    PE_MIPS_R_BASED_JMPADDR16 = 0x09UN;

/* MIPS PE relocation.  */

var PE_MIPS_R_ABSOLUTE = 0x0000UH,
    PE_MIPS_R_REFHALF = 0x0001UH,
    PE_MIPS_R_REFWORD = 0x0002UH,
    PE_MIPS_R_JMPADDR = 0x0003UH,
    PE_MIPS_R_REFHI = 0x0004UH,
    PE_MIPS_R_REFLO = 0x0005UH,
    PE_MIPS_R_GPREL = 0x0006UH,
    PE_MIPS_R_LITERAL = 0x0007UH,
    PE_MIPS_R_SECTION = 0x000AUH,
    PE_MIPS_R_SECREL = 0x000BUH,
    PE_MIPS_R_SECRELLO = 0x000CUH,
    PE_MIPS_R_SECRELHI = 0x000DUH,
    PE_MIPS_R_JMPADDR16 = 0x0010UH,
    PE_MIPS_R_REFWORDNB = 0x0022UH,
    PE_MIPS_R_PAIR = 0x0025UH;
