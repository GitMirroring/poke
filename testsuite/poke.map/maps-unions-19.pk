type Bar = struct int<32>
{
  int a : a == 666;
};

type Foo =
  union int<32>
  {
    Bar a;
    int b;

    method get_a = (int x) int: { return a + x; }
    method get_b = (int x) int: { return b + x; }
    method _print = void: { printf "foo"; }
  };

