/* { dg-do run } */

type S = struct uint
  {
    int16 a;
    offset<int16,b> b;
  };

type U = union int
  {
    S a : a.a < 0;
    uint b : b < 10;
  };

var s = S { a=0xabcd, b=0xef12H#b };
var u = U { a = s };

